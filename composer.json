{
    "name": "mihkullorg/laravel-translatable",
    "description": "Laravel package for multilingual models with Google Cloud Translate",
    "keywords": ["laravel","translate","google-translate"],
    "require": {
        "google/cloud-translate": "^1.2",
        "php": ">=7.1.3",
        "illuminate/support": "5.6.*|5.7.*"
    },
    "require-dev": {
        "matthewbdaly/artisan-standalone": "0.0.*",
        "orchestra/testbench": "^3.7",
        "phpunit/phpunit": "^7.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Mihkel Allorg",
            "email": "mihkel@lightweber.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Mihkullorg\\Translatable\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mihkullorg\\Translatable\\TranslatableServiceProvider"
            ]
        }
    }
}
